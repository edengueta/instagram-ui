on: UserService.getToken(),\n\t\t\t},\n\t\t});\n        return res.json();\n    }\n\n    static async like(postId,userId) {\n        const res = await fetch (environment.apiUrl + '/post/'+ postId +'/likes', {\n            method: 'POST',\n\t\t\theaders: {\n\t\t\t\t'Content-Type': 'application/json',\n                 Authorization: UserService.getToken(),\n\t\t\t},\n            body: JSON.stringify({\n                userId\n            }),\n\t\t});\n    }\n    static async unlike(postId,userId) {\n        const res = await fetch (environment.apiUrl + '/post/'+ postId +'/likes/' + userId, {\n            method: 'DELETE',\n\t\t\theaders: {\n                 Authorization: UserService.getToken(),\n\t\t\t},\n\t\t});\n    }\n\n    static async createComment(postId,content) {\n        const res = await fetch (environment.apiUrl + '/post/'+ postId +'/comment', {\n            method: 'PUT',\n\t\t\theaders: {\n\t\t\t\t'Content-Type': 'application/json',\n                 Authorization: UserService.getToken(),\n\t\t\t},\n            body: JSON.stringify({\n                content\n            }),\n\t\t});\n        return res.json();\n    }\n    \n    static async getComments(postId) {\n        const res = await fetch (environment.apiUrl + '/post/'+ postId +'/comment', {\n            headers: {\n                Authorization: UserService.getToken()\n            }\n        });\n        return res.json();\n    }\n\n}","/Users/eden/Desktop/Netcraft-FullStack-Course/Evyatar/final-project/instagram-ui/src/Header/HeaderAvatar/HeaderAvatar.js",[],"/Users/eden/Desktop/Netcraft-FullStack-Course/Evyatar/final-project/instagram-ui/src/common/Avatar/Avatar.js",[],"/Users/eden/Desktop/Netcraft-FullStack-Course/Evyatar/final-project/instagram-ui/src/user-context.js",[],"/Users/eden/Desktop/Netcraft-FullStack-Course/Evyatar/final-project/instagram-ui/src/Header/Menu/Menu.js",[],"/Users/eden/Desktop/Netcraft-FullStack-Course/Evyatar/final-project/instagram-ui/src/PostCreate/PostCreate.js",[]