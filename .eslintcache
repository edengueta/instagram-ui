e=\"post-details col col-lg-4 \">\n                    <div className=\"header\">\n                        <Avatar size=\"md\" image={post.user.avatar} link={post.user.username}/>\n                        <div className=\"username-date\">\n                            <Username username={post.user.username}/>\n                            <CreatedAt link={\"/post/\"+post._id} date={post.createdAt}/>\n                        </div>\n                        <div className=\"likes\">\n                            <PostLike postId={post._id} likesCount={post.likes.length}/>\n                        </div>\n                    </div>\n                    { post.caption &&\n                        <div className=\"caption\">\n                            <span>{post.caption}</span>\n                        </div>\n                    }\n                    <div onScroll={onScroll} className=\"comments\">\n                        <Comments postId={post._id}/>\n                    </div>\n                    { isFade && <div className={\"fade-out\"}></div> }\n                </div>\n            </article>\n\n        )}\n        </>\n    );\n}\n\nexport default PostPage;","/Users/eden/Desktop/Netcraft-FullStack-Course/Evyatar/final-project/instagram-ui/src/Profile/Profile.js",[],"/Users/eden/Desktop/Netcraft-FullStack-Course/Evyatar/final-project/instagram-ui/src/Profile/PostImage/PostImage.js",[],"/Users/eden/Desktop/Netcraft-FullStack-Course/Evyatar/final-project/instagram-ui/src/Profile/ProfileHeader/ProfileHeader.js",[],"/Users/eden/Desktop/Netcraft-FullStack-Course/Evyatar/final-project/instagram-ui/src/Search/Search.js",[],"/Users/eden/Desktop/Netcraft-FullStack-Course/Evyatar/final-project/instagram-ui/src/Search/SearchResult/SearchResult.js",[],"/Users/eden/Desktop/Netcraft-FullStack-Course/Evyatar/final-project/instagram-ui/src/common/Username/Username.js",[],"/Users/eden/Desktop/Netcraft-FullStack-Course/Evyatar/final-project/instagram-ui/src/common/PostLike/PostLike.js",["153"],"import React, { useContext, useEffect, useState } from 'react';\nimport './PostLike.scss';\nimport { AiFillHeart, AiOutlineHeart } from 'react-icons/ai';\nimport { PostService } from '../../services/post.service';\nimport { UserContext } from '../../user-context';\n        \n\nfunction PostLike({postId, likesCount}) {\n        \n    const { user } = useContext( UserContext );\n    const userId= user._id;\n\n    const [isLiked, setLike] = useState(false);\n    const [counter, setCounter] = useState(likesCount);\n\n\n    useEffect(()=> {\n\n        async function getIsLiked() {\n            try{\n                const res=await PostService.isLiked(postId, userId);\n                if (!res) {\n                    setLike(false);\n                    return;\n                }\n                setLike(true);\n            }catch(err){\n                console.log(err)\n            }\n        }\n        getIsLiked();\n        \n    },[postId]);\n\n    \n    function likeToggle(){\n        if (!isLiked) {\n           addLike(); \n        } else {\n            removeLike()\n        };\n        setLike(!isLiked);\n    }\n\n    async function addLike(){\n        try{\n            await PostService.like(postId, userId);\n        }catch(err){\n            console.log(err)\n        }\n        console.log(\"added\"); \n        setCounter(counter+1);\n    }\n\n    async function removeLike(){\n        try{\n            await PostService.unlike(postId, userId);\n        }catch(err){\n            console.log(err)\n        }\n        console.log(\"removed\");\n        setCounter(counter-1);\n    }\n\n    return (\n            <div className=\"PostLike\">\n                <span onClick={likeToggle}>\n                    {!isLiked && <AiOutlineHeart className=\"like-button unliked\"/>}\n                    {isLiked && <AiFillHeart className=\"like-button liked\"/>}\n                </span>\n    \n                <span className=\"counter\">{counter}</span>\n            </div>     \n    );\n}\n\nexport default PostLike;\n","/Users/eden/Desktop/Netcraft-FullStack-Course/Evyatar/final-project/instagram-ui/src/common/Comments/Comments.js",["154"],"import \"./Comments.scss\";\nimport CommentAdd from './CommentAdd/CommentAdd';\nimport Comment from \"./Comment/Comment\";\nimport { useEffect, useState } from \"react\";\nimport { PostService } from \"../../services/post.service\";\n\n\nfunction Comments({postId}) {\n\n    const [comments,setComments] = useState([]);\n\n\n    useEffect(()=>{\n        async function getComments(){\n            try{\n                const commentsArr = await PostService.getComments(postId);\n                setComments(commentsArr);\n            }catch(err){\n                console.log(err)\n            } \n        }\n        getComments()\n        \n    },[postId]);\n\n    function onCommentAdd(comment){\n        setComments([...comments, comment])\n    }\n    function isManyComments(){\n        if (comments.length > 8) {\n            return true\n        }\n        return false\n    }\n\n    return (\n        <div className=\"Comments\">\n            <CommentAdd postId={postId} onCommentAdd={onCommentAdd}/>\n            { comments.map( comment => <Comment key={comment._id} comment={comment}/> ) }\n        </div>\n    );\n}\n\nexport default Comments;\n\n\n\n\n\n","/Users/eden/Desktop/Netcraft-FullStack-Course/Evyatar/final-project/instagram-ui/src/common/Comments/CommentAdd/CommentAdd.js",[],"/Users/eden/Desktop/Netcraft-FullStack-Course/Evyatar/final-project/instagram-ui/src/common/Comments/Comment/Comment.js",[],"/Users/eden/Desktop/Netcraft-FullStack-Course/Evyatar/final-project/instagram-ui/src/Feed/Post/Post.js",[],"/Users/eden/Desktop/Netcraft-FullStack-Course/Evyatar/final-project/instagram-ui/src/common/CreatedAt/CreatedAt.js",[],"/Users/eden/Desktop/Netcraft-FullStack-Course/Evyatar/final-project/instagram-ui/src/common/FollowButton/FollowButton.js",["155"],{"ruleId":"156","replacedBy":"157"},{"ruleI