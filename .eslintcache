\n        }\n        getIsLiked();\n        \n    },[postId]);\n\n    \n    function likeToggle(){\n        if (!isLiked) {\n           addLike(); \n        } else {\n            removeLike()\n        };\n        setLike(!isLiked);\n    }\n\n    async function addLike(){\n        try{\n            await PostService.like(postId, user._id);\n        }catch(err){\n            console.log(err)\n        }\n        console.log(\"added\"); \n        setCounter(counter+1);\n    }\n\n    async function removeLike(){\n        try{\n            await PostService.unlike(postId, user._id);\n        }catch(err){\n            console.log(err)\n        }\n        console.log(\"removed\");\n        setCounter(counter-1);\n    }\n\n    return (\n            <div className=\"PostLike\">\n                <span onClick={likeToggle}>\n                    {!isLiked && <AiOutlineHeart className=\"like-button unliked\"/>}\n                    {isLiked && <AiFillHeart className=\"like-button liked\"/>}\n                </span>\n    \n                <span className=\"counter\">{counter}</span>\n            </div>     \n    );\n}\n\nexport default PostLike;\n","/Users/eden/Desktop/Netcraft-FullStack-Course/Evyatar/final-project/instagram-ui/src/common/Comments/Comments.js",[],"/Users/eden/Desktop/Netcraft-FullStack-Course/Evyatar/final-project/instagram-ui/src/common/Comments/CommentAdd/CommentAdd.js",[],"/Users/eden/Desktop/Netcraft-FullStack-Course/Evyatar/final-project/instagram-ui/src/common/Comments/Comment/Comment.js",[],"/Users/eden/Desktop/Netcraft-FullStack-Course/Evyatar/final-project/instagram-ui/src/Feed/Post/Post.js",[],"/Users/eden/Desktop/Netcraft-FullStack-Course/Evyatar/final-project/instagram-ui/src/common/CreatedAt/CreatedAt.js",[],"/Users/eden/Desktop/Netcraft-FullStack-Course/Evyatar/final-project/instagram-ui/src/common/FollowButton/FollowButton.js",[],"/Users/eden/Desktop/Netcraft-FullStack-Course/Evyatar/final-project/instagram-ui/src/environments/production.js",[],{"r