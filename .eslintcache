\n        };\n        setLike(!isLiked);\n        console.log(isLiked)\n    }\n\n    async function addLike(){\n        try{\n            const res=await PostService.like(postId);\n            console.log(\"added\"); \n            setCounter(res.likes.length);\n        }catch(err){\n            console.log(err)\n        }\n    }\n\n    async function removeLike(){\n        try{\n            const res=await PostService.unlike(postId, user._id);\n            console.log(\"removed\");\n            setCounter(res.likes.length);\n        }catch(err){\n            console.log(err)\n        }\n\n    }\n\n    return (\n            <div className=\"PostLike\">\n                <span onClick={likeToggle}>\n                    {!isLiked && <AiOutlineHeart className=\"like-button unliked\"/>}\n                    {isLiked && <AiFillHeart className=\"like-button liked\"/>}\n                </span>\n    \n                <span className=\"counter\">{counter}</span>\n            </div>     \n    );\n}\n\nexport default PostLike;\n","/Users/eden/Desktop/Netcraft-FullStack-Course/Evyatar/final-project/instagram-ui/src/common/Comments/Comments.js",[],"/Users/eden/Desktop/Netcraft-FullStack-Course/Evyatar/final-project/instagram-ui/src/common/Comments/CommentAdd/CommentAdd.js",[],"/Users/eden/Desktop/Netcraft-FullStack-Course/Evyatar/final-project/instagram-ui/src/common/Comments/Comment/Comment.js",[],"/Users/eden/Desktop/Netcraft-FullStack-Course/Evyatar/final-project/instagram-ui/src/Feed/Post/Post.js",[],"/Users/eden/Desktop/Netcraft-FullStack-Course/Evyatar/final-project/instagram-ui/src/common/CreatedAt/CreatedAt.js",[],"/Users/eden/Desktop/Netcraft-FullStack-Course/Evyatar/final-project/instagram-ui/src/common/FollowButton/FollowButton.js",[],"/Users/eden/Desktop/Netcraft-FullStack-Course/Evyatar/final-project/instagram-ui/src/environments/production.js",[],"/Users/eden/Desktop/Netcraft-FullStack-Course/Evyatar/final-project/instagram-ui/src/Profile/ProfileHeader/UploadAvatar/U